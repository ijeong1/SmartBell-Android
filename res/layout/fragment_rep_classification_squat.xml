<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <TextView
        android:id="@+id/rep_category_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Categorize rep "
        android:textAppearance="?android:attr/textAppearanceLarge" />

    <CheckBox
        android:id="@+id/golden_squat"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Golden" />
    
    <CheckBox
        android:id="@+id/correct_check_box_squat"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Correct" />

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Back errors" />
    
    <CheckBox
        android:id="@+id/chin_tuck"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Chin not tucked / head not neutral" />
    
    <CheckBox
        android:id="@+id/upper_back_round_check_box"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Upper back round" />
    
    <CheckBox
        android:id="@+id/lower_back_round_check_box"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Lower back round / butt wink" />
    
    <CheckBox
        android:id="@+id/over_extension_check_box"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Over extension / too vertical" />      
    
    <CheckBox
        android:id="@+id/chase_back_check_box"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Hips out / Chasing with back" />
    
    <CheckBox
        android:id="@+id/hips_roll_check_box"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Hips roll under spine" />
    
    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Other errors" />
    
    <CheckBox
        android:id="@+id/not_parallel_check_box"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Did not get to parallel" />
    
    <CheckBox
        android:id="@+id/stand_up_check_box"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Did not stand all the way up" />
    	
    <CheckBox
        android:id="@+id/knees_out_check_box"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Knees not spread out" />
    
    <CheckBox
        android:id="@+id/heels_up_check_box"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Heels coming up off of the ground" />
    
    <CheckBox
        android:id="@+id/wrists_roll_check_box_squat"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Wrists rolled back" />
    
    <CheckBox
        android:id="@+id/incomplete_check_box_squat"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Did not complete rep" />
    
    <Button
        android:id="@+id/continue_button_squat"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Continue" />

</LinearLayout>