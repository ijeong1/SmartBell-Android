<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="match_parent" android:layout_height="match_parent"
	android:orientation="vertical"
	android:id="@+id/select_athlete_fragment">

	<TextView android:id="@+id/select_athlete_text" android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:text="Select Athlete"
		android:textAppearance="?android:attr/textAppearanceLarge" />

	<Button android:id="@+id/new_athlete_button" android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:text="New Athlete" />

		
	<LinearLayout android:id="@+id/athlete_list"
		android:layout_width="match_parent" android:layout_height="wrap_content"
		android:orientation="vertical" />


</LinearLayout>
